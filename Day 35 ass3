#include <stdio.h>
int main() {
   int n,i,a[10],max,min,diff;
   scanf("%d",&n);
   for(i=0;i<n;i++){
       scanf("%d",&a[i]);
   }
   max=a[0];
   min=a[0];
   printf("Elements of array:");
   for(i=0;i<n;i++){
       printf("%d\n",a[i]);
   }
       for(i=0;i<n;i++){
        if(a[i]>max){
           max=a[i];
       }
       if(a[i]<min){
           min=a[i];
       }   
       }
       diff=max-min;
    printf("\ndifference b/w max ele and min ele=%d",diff);
    return 0;
}
