#include <stdio.h>

int main()
{
	int rows;
	printf("Enter Square Left Shift Numbers Rows = ");
	scanf("%d", &rows);
	printf("The Square Pattern of Left Shift Numbers\n");
       for (int i = 1; i <= rows; i++)
	{
		for (int j = i; j <= rows; j++)
		{
			printf("%d ", j);
		}
		for (int k = 1; k < i; k++)
		{
			printf("%d ", k);
		}
		printf("\n");
	}
}
